<ul>
  {{#each changeset.errors as |errorSet|}}
    {{#each errorSet.validation as |error|}}
      <li>{{error}}</li>
    {{/each}}
  {{/each}}
</ul>

<form>
  <div class="form-group">
    <label for="clientEmail">Client email</label>
    {{input value=changeset.clientEmail update=(action (mut changeset.clientEmail)) class="form-control"}}
  </div>
  <div class="form-group">
    <!-- <label for=""></label>-->
    {{date-range-picker
      label='Start at - End at'
      start=changeset.startAt
      end=changeset.endAt
    }}
  </div>
</form>

<button class="btn btn-primary" {{action 'save'}}>
  Save
</button>
<button class="btn btn-primary" {{action 'cancel'}}>
  Cancel
</button>
